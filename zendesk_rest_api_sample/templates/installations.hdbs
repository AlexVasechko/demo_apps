<p>{{t "list_app"}}</p>

<ul id="installations">
  {{#each installations}}
  <li class="{{enabled}} {{#if ../inline}}inline{{/if}}">
    <div>({{id}}) {{settings.title}}<br />
    {{#if enabled}}
      <a class="deactivate" data-id="{{id}}">{{t "deactivate"}}</a>
    {{else}}
      <a class="activate" data-id="{{id}}">{{t "activate"}}</a>
    {{/if}}
      or
      <a class="delete" data-id="{{id}}">{{t "delete"}}</a>
    </div>
  </li>
  {{/each}}
</ul>
