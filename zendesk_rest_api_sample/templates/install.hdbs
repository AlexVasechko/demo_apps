<form class="install-form">
  <ul class="parameters">
  {{#each parameters}}
    <li>
      <label for="{{name}}">{{name}}</label>
      <input name="{{name}}" type="text" {{#if required}}{{t "required"}}{{/if}}>
    </li>
  {{/each}}
  </ul>
  <p>
    <button class="install-button btn btn-inverse" data-id="{{id}}">
      {{t "install"}}
    </button>
  </p>
</form>
