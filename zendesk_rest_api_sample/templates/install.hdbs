Alright, let's get this app installed.

<form id="install">
  <ul id="parameters">
  {{#each parameters}}
    <li>
      <label for="{{name}}">{{name}}</label>
      <input name="{{name}}" type="text" {{#if required}}required{{/if}} />
    </li>
  {{/each}}
  </ul>
  <p><button class="install btn btn-inverse" data-id="{{id}}">Install</button></p>
</form>
