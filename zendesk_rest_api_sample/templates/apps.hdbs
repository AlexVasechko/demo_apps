<ul class="{{#unless inline}}scroll_list {{/unless}}available_apps">
  {{#each apps}}
  <li class="{{#if ../inline}}inline{{/if}}">
    <div class="app_box">
      <img class="app_inline_logo" src="/api/v2/apps/{{id}}/assets/logo-small.png" alt="{{t "logo"}}">
      ({{id}}) {{name}}<br>
      <a class="install" data-id="{{id}}">{{t "install"}}</a>
    </div>
  </li>
  {{/each}}
</ul>
