<div class="modal hide fade my_modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
    <h3 class="my_modal_label"></h3>
  </div>
  <div class="modal-body">
    <div class="progress progress-striped active">
      <div class="bar"></div>
      <span class="sr-only"></span>
    </div>
    <div class="alert alert-block">
      <h4>Warning!</h4>
      <span>{{t "reject_msg_no_comment"}}</span>
    </div>
  </div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal" aria-hidden="true">{{t "ok_button"}}</button>
  </div>
</div>
<div class="tickets_list">
  <div class="tickets_list_header"><h5></h5></div>
  <div class="tickets_list_body">
    <ul>
      {{#ticketsInfo}}
      <li>
        <a href="{{url}}">{{subject}}</a>
      </li>
      {{/ticketsInfo}}
    </ul>
  </div>
  <div class="pagination pgination-centered">
    <ul>
      <!-- Use of Glyphicons http://getbootstrap.com/2.3.2/base-css.html#icons -->
      <li class="active"><a class="prev" href="#"><i class="icon-backward"></i></a></li>
      {{#pages}}
        <li class="active pagi {{paginator}}"><a class="page_number" href="#">{{number}}</a></li>
      {{/pages}}
      <li class="active"><a class="next" href="#"><i class="icon-forward"></i></a></li>
    </ul>
  </div>
</div>
